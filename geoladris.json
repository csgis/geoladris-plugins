{
  "default-conf": {
    "ui": {},
    "ui-sliding-div": {
      "duration": 500
    }
  },
  "requirejs": {
    "paths": {
      "datatables.net": "jslib/datatables.net",
      "datatables.net-buttons": "jslib/datatables.net-buttons",
      "datatables.net-colVis": "jslib/datatables.net-colVis",
      "popper.js": "node_modules/popper.js/dist/umd/popper",
      "tooltip.js": "node_modules/tooltip.js/dist/umd/tooltip",
      "moment": "node_modules/moment/min/moment.min",
      "nouislider": "node_modules/nouislider/distribute/nouislider.min",
      "pikaday": "node_modules/pikaday/pikaday",
      "pikaday.jquery": "node_modules/pikaday/plugins/pikaday.jquery",
      "sortable": "node_modules/sortablejs/Sortable.min",
      "typeahead": "node_modules/typeahead.js/dist/typeahead.jquery.min"
    },
    "shim": {
      "pikaday.jquery": {
        "deps": ["pikaday"]
      },
      "pikaday": {
        "deps": ["moment"]
      }
    }
  },
  "css": [
    "node_modules/pikaday/css/pikaday.css",
    "node_modules/nouislider/distribute/nouislider.min.css"
  ]
}
