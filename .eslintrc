{
  "extends": "eslint-config-airbnb-es5",
  "rules" : {
    "indent" : ["error", "tab"],
    "space-before-function-paren": ["error", {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "ignore"
    }],
    "func-names": 0
  },
  "env" : {
    "browser" : true,
    "mocha" : true
  }
}
